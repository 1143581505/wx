/* components/MyInput/MyInput.wxss */
.search{
  border:1px solid #777;
  border-radius: 40px;
  height: 60rpx;
  margin: 10rpx 30rpx 30rpx 30rpx;
  display:flex;
}
.search .search_input{
  display: inline;
  width:75%;
  margin: auto 0;
  font-size: 25rpx;
}
.search .search_img{
  height: 60rpx;
  width: 60rpx;
  display: inline-flex;
  margin-left: 20rpx;
  border-radius: 10rpx;
}
.search .refresh_img{
  height: 40rpx;
  width: 40rpx;
  display: inline-flex;
  margin:auto 20rpx;
  border-radius: 10rpx;
  border-radius: 50%;
}
